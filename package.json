    "dependencies": {
        "axios": "^1.11.0",
        "cheerio": "^1.1.2",
        "dayjs": "^1.11.18",
        "es-main": "^1.4.0",
        "fast-xml-parser": "^4.5.3",
        "feed": "^4.2.2",
        "handlers.js": "0.1.2-1",
        "lru-cache": "^10.4.3",
        "mathjs": "^12.4.3",
        "sync-request": "^6.1.0",
        "typescript": "^5.9.2"
    },
    "devDependencies": {
        "@dotenvx/dotenvx": "^1.49.0",
        "@eslint/js": "^9.34.0",
        "@types/jest": "^29.5.14",
        "@types/node": "^20.19.11",
        "@types/tape": "^5.8.1",
        "@typescript-eslint/eslint-plugin": "^7.18.0",
        "@typescript-eslint/parser": "^7.18.0",
        "esbuild": "^0.23.1",
        "eslint": "~9.34.0",
        "eslint-config-prettier": "^9.1.2",
        "eslint-plugin-prettier": "^5.5.4",
        "globals": "^15.15.0",
        "husky": "^9.1.7",
        "jest": "^29.7.0",
        "prettier": "^3.6.2",
        "ts-jest": "^29.4.1",
        "ts-node": "^10.9.2",
        "tsc-alias": "^1.8.16",
        "tslib": "^2.8.1",
        "tsx": "^4.20.5",
        "typescript-eslint": "^8.41.0"
    },
    "type": "module",
    "scripts": {
        "postinstall": "husky",
        "format": "prettier --write \"**/*.{ts,json,md}\" ",
        "lint": "eslint \"{src,test}/**/*.ts\" --fix",
        "build": "yarn clean && esbuild src/index.ts --minify --entry-names=[name] --format=cjs --platform=node --bundle --outdir=dist --external:sync-request --define:\"globalThis.esBuilt=true\" --define:globalThis.GITBUILD=\\\"$(git rev-parse --short HEAD)\\\" --define:globalThis.BUILDTIME=\\\"$(date -Iseconds)\\\" && mv dist/index.js dist/index.cjs",
        "clean": "rm -rf dist/[!readme.md]*",
        "dev": "dotenvx run -- tsx watch src/index.ts",
        "dev:production": "dotenvx run --env-file=.env.production.local --env-file=.env -- tsx watch src/index.ts",
        "start": "yarn build && node dist/index.cjs",
        "test": "NODE_OPTIONS=--experimental-vm-modules jest",
        "test:coverage": "yarn test --collectCoverage"
    }
}
